<div class='user-post'>
  <h4 class='post-title'><%= link_to(post.title, user_post_path(@user, post), class: "user-name") %></h4>
  <p class='post-text'>
    <%= post.text %>.
  </p>
  <p class='statics'>Comments: <span><%= post.comments.count %></span>, Likes: <span><%= post.likes.count %></span></p>
</div>